{
    "$defs": {
        "BaseSequencingAttributes": {
            "additionalProperties": false,
            "description": "Base attributes shared across all sequencing types",
            "properties": {
                "CHECKSUM": {
                    "description": "Checksum for data integrity verification",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "COMPONENT": {
                    "description": "Category of metadata (e.g., Bulk WES Level 1, scRNA-seq Level 2, etc.)",
                    "type": "string"
                },
                "FILENAME": {
                    "description": "Name of the file",
                    "pattern": "^.+[\\\\/]\\S*$",
                    "type": "string"
                },
                "FILE_FORMAT": {
                    "description": "Format of the file (e.g., fastq, bam, vcf, h5ad)",
                    "type": "string"
                },
                "GENOME_ANNOTATION_URL": {
                    "description": "URL to genome annotation",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "GENOMIC_REFERENCE": {
                    "description": "Genomic reference used for alignment",
                    "type": "string"
                },
                "GENOMIC_REFERENCE_URL": {
                    "description": "URL to genomic reference",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "HTAN_BIOSPECIMEN_ID": {
                    "description": "HTAN Biospecimen ID of the parent biospecimen (Primary Key)",
                    "pattern": "^(HTA([1-9]|1[0-6]))_((EXT)?([0-9]\\d*|0000))_([0-9]\\d*|0000)$",
                    "type": "string"
                },
                "HTAN_DATA_FILE_ID": {
                    "description": "HTAN Data File ID (Primary Key)",
                    "pattern": "^(HTA([1-9]|1[0-6]))_((EXT)?([0-9]\\d*|0000))_([0-9]\\d*|0000)$",
                    "type": "string"
                },
                "HTAN_PARENT_ID": {
                    "description": "HTAN Parent ID - Foreign Key to parent entity (B for Biospecimen, D for data file)",
                    "pattern": "^(HTA20[0-9])(?:_0000)?(?:_\\d+)?(?:_EXT\\d+)?_(B|D)\\d{1,50}$",
                    "type": "string"
                },
                "LIBRARY_LAYOUT": {
                    "$ref": "#/$defs/LibraryLayoutEnum",
                    "description": "Library layout (paired-end or single-end)"
                },
                "LIBRARY_PREPARATION_DAYS_FROM_INDEX": {
                    "description": "Days from index for library preparation",
                    "type": [
                        "integer",
                        "null"
                    ]
                },
                "PROTOCOL_LINK": {
                    "description": "Link to sequencing protocol",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "SEQUENCING_BATCH_ID": {
                    "description": "Sequencing batch identifier",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "SEQUENCING_PLATFORM": {
                    "$ref": "#/$defs/SequencingPlatformEnum",
                    "description": "Sequencing platform used"
                },
                "TECHNICAL_REPLICATE_GROUP": {
                    "description": "Technical replicate group identifier",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "WORKFLOW_LINK": {
                    "description": "Link to workflow or command. DockStore.org recommended",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "WORKFLOW_VERSION": {
                    "description": "Major version of the workflow",
                    "type": "string"
                }
            },
            "required": [
                "LIBRARY_LAYOUT",
                "SEQUENCING_PLATFORM",
                "WORKFLOW_VERSION",
                "GENOMIC_REFERENCE",
                "HTAN_BIOSPECIMEN_ID",
                "COMPONENT",
                "FILENAME",
                "FILE_FORMAT",
                "HTAN_DATA_FILE_ID",
                "HTAN_PARENT_ID"
            ],
            "title": "BaseSequencingAttributes",
            "type": "object"
        },
