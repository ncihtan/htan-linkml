                "@type": "SlotDefinition"
              },
              {
                "name": "CLINICAL_M_STAGE",
                "required": true,
                "@type": "SlotDefinition"
              },
              {
                "name": "AJCC_STAGING_SYSTEM_EDITION",
                "required": true,
                "@type": "SlotDefinition"
              }
            ],
            "@type": "AnonymousClassExpression"
          },
          "description": "When TUMOR_STAGE has a value, staging fields become required",
          "@type": "ClassRule"
        },
        {
          "preconditions": {
            "slot_conditions": [
              {
                "name": "TUMOR_STAGE",
                "pattern": "^Stage IV.*",
                "@type": "SlotDefinition"
              }
            ],
            "@type": "AnonymousClassExpression"
          },
          "postconditions": {
            "slot_conditions": [
              {
                "name": "METASTASIS_AT_DIAGNOSIS",
                "required": true,
                "@type": "SlotDefinition"
              }
            ],
            "@type": "AnonymousClassExpression"
          },
          "description": "When TUMOR_STAGE is Stage IV, metastasis field becomes required",
          "@type": "ClassRule"
        }
      ],
      "@type": "ClassDefinition"
