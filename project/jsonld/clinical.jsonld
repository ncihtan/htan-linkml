    {
      "name": "ClinicalData",
      "definition_uri": "https://w3id.org/htan/ClinicalData",
      "description": "Container for all clinical data",
      "from_schema": "https://w3id.org/htan/clinical",
      "slots": [
        "clinicalData__COMPONENT",
        "clinicalData__HTAN_PARTICIPANT_ID",
        "clinicalData__DEMOGRAPHICS",
        "clinicalData__VITAL_STATUS",
        "clinicalData__DIAGNOSIS",
        "clinicalData__EXPOSURES",
        "clinicalData__FAMILY_HISTORY",
        "clinicalData__FOLLOW_UPS",
        "clinicalData__MOLECULAR_TESTS",
        "clinicalData__THERAPIES"
      ],
      "slot_usage": {},
      "attributes": [
        {
          "name": "COMPONENT",
          "description": "Category of metadata (e.g. Diagnosis, Biospecimen, scRNA-seq Level 1, etc.); provide the same one for all items/rows. (No CRDC Standard Available)",
          "range": "ComponentEnum",
          "required": true,
          "@type": "SlotDefinition"
        },
        {
          "name": "HTAN_PARTICIPANT_ID",
          "description": "HTAN Identifier of the research participant (caDSR:HTAN-Specific) (No CRDC Standard Available)",
          "range": "string",
          "required": true,
          "pattern": "^(HTA20[0-9])(?:_0000)?(?:_\\d+)?(?:_EXT\\d+)?_(B|D)\\d{1,50}$",
          "@type": "SlotDefinition"
        },
        {
          "name": "DEMOGRAPHICS",
          "description": "Demographic information",
          "range": "Demographics",
          "required": true,
          "@type": "SlotDefinition"
        },
        {
          "name": "VITAL_STATUS",
          "description": "Vital status information",
          "range": "VitalStatus",
          "required": true,
          "@type": "SlotDefinition"
        },
        {
          "name": "DIAGNOSIS",
          "description": "Primary diagnosis information",
          "range": "Diagnosis",
          "required": true,
          "@type": "SlotDefinition"
        },
        {
          "name": "EXPOSURES",
          "description": "Exposure history",
          "range": "Exposure",
          "required": true,
          "@type": "SlotDefinition"
        },
        {
          "name": "FAMILY_HISTORY",
          "description": "Family history of cancer",
          "range": "FamilyHistory",
          "required": true,
          "@type": "SlotDefinition"
        },
        {
          "name": "FOLLOW_UPS",
          "description": "Follow-up observations",
          "range": "FollowUp",
          "multivalued": true,
          "@type": "SlotDefinition"
        },
        {
          "name": "MOLECULAR_TESTS",
          "description": "Molecular test results",
          "range": "MolecularTest",
          "multivalued": true,
          "@type": "SlotDefinition"
        },
        {
          "name": "THERAPIES",
          "description": "Treatment information",
          "range": "Therapy",
          "multivalued": true,
          "@type": "SlotDefinition"
        }
      ],
      "class_uri": "https://w3id.org/htan/ClinicalData",
      "@type": "ClassDefinition"
    },
