htan:CoreFileAttributes a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "CoreFileAttributes" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty htan:HTAN_BIOSPECIMEN_ID ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^(HTA([1-9]|1[0-6]))_((EXT)?([0-9]\\d*|0000))$" ] ) ] ) ] ;
            owl:onProperty htan:HTAN_PARTICIPANT_ID ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty htan:FILE_FORMAT ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty htan:HTAN_BIOSPECIMEN_ID ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty htan:HTAN_PARTICIPANT_ID ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty htan:HTAN_PARTICIPANT_ID ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty htan:HTAN_DATA_FILE_ID ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty htan:COMPONENT ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty htan:HTAN_DATA_FILE_ID ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^(HTA([1-9]|1[0-6]))_((EXT)?([0-9]\\d*|0000))_([0-9]\\d*|0000)$" ] ) ] ) ] ;
            owl:onProperty htan:HTAN_BIOSPECIMEN_ID ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^(HTA20[0-9])(?:_0000)?(?:_\\d+)?(?:_EXT\\d+)?_(B|D)\\d{1,50}$" ] ) ] ) ] ;
            owl:onProperty htan:HTAN_PARENT_ID ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty htan:FILE_FORMAT ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty htan:HTAN_PARENT_ID ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty htan:COMPONENT ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^.+[\\\\/]\\S*$" ] ) ] ) ] ;
            owl:onProperty htan:FILENAME ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty htan:FILENAME ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty htan:COMPONENT ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty htan:HTAN_PARENT_ID ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^(HTA([1-9]|1[0-6]))_((EXT)?([0-9]\\d*|0000))_([0-9]\\d*|0000)$" ] ) ] ) ] ;
            owl:onProperty htan:HTAN_DATA_FILE_ID ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty htan:FILENAME ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty htan:FILE_FORMAT ] ;
    skos:definition "Universal attributes that apply to all file-based data in HTAN" ;
    skos:inScheme htan:core .

